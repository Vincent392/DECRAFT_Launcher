<UserControl x:Class="DeCraftLauncher.UIControls.JarLibListEntry"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DeCraftLauncher.UIControls"
             mc:Ignorable="d" >
    <StackPanel Orientation="Horizontal">
        <CheckBox Style="{DynamicResource MainCheckBoxStyle}" x:Name="checkbox_jarlibenabled" Content="" VerticalContentAlignment="Center"/>
        <Label x:Name="label_maintext" Content="Jar lib file name here" Margin="10,0" Foreground="White" />
    </StackPanel>
</UserControl>
