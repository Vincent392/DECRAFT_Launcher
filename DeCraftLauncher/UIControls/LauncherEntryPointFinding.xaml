<UserControl x:Class="DeCraftLauncher.UIControls.LaunchEntryPointFinding"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DeCraftLauncher.UIControls"
             mc:Ignorable="d" Height="120" Width="516">
    <Grid>
        <Label x:Name="label_classname" Content="Searching classes for entry points..." Margin="10,10,10,0" VerticalAlignment="Top" FontSize="25" Foreground="White"/>
        <Label x:Name="progress_pct" Content="0%" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top" Width="101" Foreground="White"/>
        <ProgressBar x:Name="search_progress" HorizontalAlignment="Left" Height="23" Margin="116,83,0,0" VerticalAlignment="Top" Width="390" Background="{x:Null}" BorderBrush="#FF353535" Foreground="White"/>

    </Grid>
</UserControl>
