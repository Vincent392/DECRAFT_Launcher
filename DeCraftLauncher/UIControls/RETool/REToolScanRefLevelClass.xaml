<UserControl x:Class="DeCraftLauncher.UIControls.RETool.REToolScanRefLevelClass"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DeCraftLauncher.UIControls.RETool"
             mc:Ignorable="d" >
    <StackPanel Orientation="Vertical">
        <StackPanel Orientation="Horizontal">
            <Button x:Name="btn_open" Style="{StaticResource ButtonMainStyle}" MinWidth="40" Height="20" Content="Open" Click="btn_open_Click"/>
            <Label x:Name="label_classname" Margin="10,-2" Foreground="White" Content="class/name/Here"/>
        </StackPanel>
        <StackPanel x:Name="panel_methods" Margin="30,0,0,0" Orientation="Vertical" Visibility="Collapsed">
            
        </StackPanel>
    </StackPanel>
</UserControl>
